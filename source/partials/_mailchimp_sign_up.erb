  </fieldset>

  <div class="c-form-field">
    <label class="u-hide-visually"
           for="mce-EMAIL">Email Address</label>
    <input id="mce-EMAIL"
           name="EMAIL"
           type="email"
           placeholder="Email Address"
           autocomplete="email"
           required>
